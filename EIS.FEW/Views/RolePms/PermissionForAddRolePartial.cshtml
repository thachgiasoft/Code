@Html.DevExpress().GridView(
    settings =>
    {
        settings.Name = "gvObject";
        settings.KeyFieldName = "objectid";
        settings.CallbackRouteValues = new { Controller = "RolePms", Action = "PermissionForAddRolePartial" };
        settings.CustomActionRouteValues = new { Controller = "RolePms", Action = "PermissionForAddRolePartialForSearch" };
        settings.Width = Unit.Percentage(80);
        settings.SettingsPager.PageSizeItemSettings.Visible = true;
        settings.SettingsPager.PageSizeItemSettings.Items = new string[] { "20", "30", "40", "50", "100" };
        settings.CommandColumn.FixedStyle = GridViewColumnFixedStyle.Left;
        settings.Settings.VerticalScrollableHeight = 600;
        settings.Settings.VerticalScrollBarMode = ScrollBarMode.Auto;
        settings.Settings.HorizontalScrollBarMode = ScrollBarMode.Auto;
        settings.SettingsPager.PageSize = 20;
        settings.SettingsBehavior.AllowSort = false;
        settings.ClientSideEvents.EndCallback = "OnEndCallback";
        settings.SettingsBehavior.ColumnResizeMode = ColumnResizeMode.Control;
        settings.ControlStyle.CssClass = "fixTopPixel";
        settings.Settings.ShowFilterRow = true;
        settings.Settings.ShowFilterRowMenu = true;

        settings.Columns.Add(col =>
        {
            col.FieldName = "objectid";
            col.Caption = "objectid";
            col.PropertiesEdit.EnableClientSideAPI = true;
            col.Width = 20;
            col.HeaderStyle.HorizontalAlign = HorizontalAlign.Left;
            col.CellStyle.Paddings.PaddingLeft = 5;
            col.FixedStyle = GridViewColumnFixedStyle.Left;
            col.Visible = false;
        });

        settings.Columns.Add(s =>
        {
            s.Caption = @"STT";
            s.ToolTip = "STT";
            s.EditFormSettings.Visible = DefaultBoolean.False;
            s.PropertiesEdit.EnableClientSideAPI = true;
            s.CellStyle.HorizontalAlign = HorizontalAlign.Left;
            s.HeaderStyle.HorizontalAlign = HorizontalAlign.Left;
            s.Width = 35;
            s.FixedStyle = GridViewColumnFixedStyle.Left;
        });
        settings.CustomColumnDisplayText = (send, e) =>
        {
            if (e.Column.Caption == "STT")
            {
                e.DisplayText = (e.VisibleRowIndex + 1).ToString();
            }
        };

        settings.Columns.Add(col =>
        {
            col.FieldName = "name";
            col.Caption = "Chức năng";
            col.PropertiesEdit.EnableClientSideAPI = true;
            col.Width = 300;
            col.HeaderStyle.HorizontalAlign = HorizontalAlign.Left;
            col.CellStyle.Paddings.PaddingLeft = 5;
            col.CellStyle.Font.Bold = true;
            col.FixedStyle = GridViewColumnFixedStyle.Left;
            col.Settings.AutoFilterCondition = AutoFilterCondition.Contains;
            col.ColumnType = MVCxGridViewColumnType.TextBox;
            col.SetDataItemTemplateContent(c =>
            {
                Html.DevExpress().Button(s =>
                {
                    s.Name = "btn_" + c.Column.FieldName + "_" + c.ItemIndex;
                    s.ControlStyle.CssClass = "mg-left";
                    var chucNang = Convert.ToString(DataBinder.Eval(c.DataItem, "name"));
                    var pmsList = "";
                    for (var i = 1; i < 11; i++)
                    {
                        pmsList += Convert.ToString(DataBinder.Eval(c.DataItem, string.Format("pms{0}", i))) + ";";
                    }
                    pmsList = pmsList.Substring(0, pmsList.Length - 1);
                    s.Text = chucNang;
                    s.Styles.Style.Font.Bold = true;
                    s.RenderMode = ButtonRenderMode.Link;
                    s.Styles.Style.CssClass = "nonUnderlineHyperlink";
                    s.ClientSideEvents.Click = "function (s, e){ OnSelectAllFunctionPms(\"" + pmsList.Trim() + "\",\"" + c.ItemIndex + "\");}";
                }).Render();
            });
        });

        settings.Columns.Add(col =>
        {
            col.FieldName = "pms1";
            col.Settings.AllowAutoFilter = DefaultBoolean.False;
            col.Caption = "Thêm";
            col.PropertiesEdit.EnableClientSideAPI = true;
            col.Width = 70;
            col.HeaderStyle.HorizontalAlign = HorizontalAlign.Center;
            col.CellStyle.Paddings.PaddingLeft = 5;
            col.CellStyle.HorizontalAlign = HorizontalAlign.Left;
            col.ColumnType = MVCxGridViewColumnType.CheckBox;

            col.SetDataItemTemplateContent(c =>
            {
                Html.DevExpress().CheckBox(checkboxSettings =>
                {
                    //checkboxSettings.Enabled=(bool)Session["statusEnableCheckBox"];
                    checkboxSettings.Name = "chk_" + c.Column.FieldName + "_" + c.ItemIndex;
                    checkboxSettings.Properties.DisplayImageChecked.IconID = IconID.ContentCheckbox16x16;
                    checkboxSettings.ControlStyle.CssClass = "mg-left";
                    var pms = Convert.ToString(DataBinder.Eval(c.DataItem, "pms1Des"));
                    var pmsName = Convert.ToString(DataBinder.Eval(c.DataItem, "pms1"));
                    //checkboxSettings.Text = pms;

                    var checkVisible = (!string.IsNullOrEmpty(pms)) ? true : false;
                    if (checkVisible == false)
                        checkboxSettings.ClientVisible = false;
                    else
                    {
                        var subName = pms.Substring(0, 1);
                        var CheckReal = (subName == " ") ? true : false;
                        checkboxSettings.Checked = CheckReal;
                        checkboxSettings.Properties.ClientSideEvents.CheckedChanged = "function (s, e){ OnCheckedChangedPms(\"" + pmsName.Trim() + "\",s.GetChecked());}";
                    }
                }).Render();
            });
            col.SetHeaderTemplateContent(c =>
            {
                Html.DevExpress().CheckBox(settingsCheckBox =>
                {
                    settingsCheckBox.Name = "cbSelectAll";
                    settingsCheckBox.Properties.DisplayImageChecked.IconID = IconID.ContentCheckbox16x16;
                    settingsCheckBox.Properties.ClientSideEvents.CheckedChanged = "function (s, e){ OnCheckedAllPms(s,e);}";
                    settingsCheckBox.Text = "Thêm";
                    settingsCheckBox.ControlStyle.CssClass = "fixText";

                }).Render();
            });
        });

