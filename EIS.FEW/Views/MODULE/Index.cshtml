@model System.Collections.IEnumerable
@{
    ViewBag.Title = "EIS v" + EIS.FEW.RouteConfig.Version + " | Quản lý Module";
    Layout = "~/Views/Shared/_mainLayout.cshtml";
}
@section HeadResources{
    @Html.Partial("HeadResourcesPartial")
}

<style>
    .dxgvTable_EIS {
        border-right: 1px solid #c0c0c0;
    }

    #gvEditing_tcFooterRow {
        color: red !important;
    }

    .btnSearch:hover {
        background-image: url('/Content/Images/Icon-flat/Search-over.png');
        width: 32px;
        height: 32px;
    }

    .btnSearch:active {
        background-image: url('/Content/Images/Icon-flat/Search.png');
        width: 32px;
        height: 32px;
    }

    .dxgvSearchPanel_EIS {
        float: left;
    }

    .height_10 {
        height: 10px;
        clear: both;
    }
</style>
<script type="text/javascript">
    $(document).ready(function () {
        $("#editMode").change(function () {
            gvEditing.PeformCallback({ editMode: $("#editMode").val() });
        });
    });
</script>
<div class="height_10"></div>
@Html.DevExpress().RoundPanel(settings =>
       {
           settings.Name = "roundPanel";
           settings.AllowCollapsingByHeaderClick = false;

           settings.ShowCollapseButton = false;
           settings.EnableAnimation = false;
           settings.HeaderText = "QUẢN LÝ MODULE";
           settings.Width = Unit.Percentage(80);
       }).GetHtml()
<div class="height_10"></div>
@Html.Partial("MODULEPartial", Model)
<div class="height_10"></div>
