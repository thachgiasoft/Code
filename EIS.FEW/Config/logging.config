<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" requirePermission="false"/>
  </configSections>

  <log4net debug="true">

    <appender name="rollingFile" type="log4net.Appender.RollingFileAppender,log4net">
      <param name="File" value="log/log.txt" />
      <param name="AppendToFile" value="true" />
      <encoding value="utf-8" />
      <maximumFileSize value="100KB" />
      <maxSizeRollBackups value="10" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyy.MM.dd" />
      <param name="StaticLogFileName" value="true" />
      <param name="CountDirection" value="1" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%date %level %logger - %message %newline" />
      </layout>
    </appender>
    <appender name="gdAppender" type="log4net.Appender.RollingFileAppender,log4net">
      <param name="File" value="log/LogGD/log.txt" />
      <param name="AppendToFile" value="true" />
      <encoding value="utf-8" />
      <maximumFileSize value="100KB" />
      <maxSizeRollBackups value="10" />
      <param name="RollingStyle" value="Date" />
      <param name="DatePattern" value="yyyy.MM.dd" />
      <param name="StaticLogFileName" value="true" />
      <param name="CountDirection" value="1" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%date %level %logger - %message %newline" />
      </layout>
    </appender>

    <root>
      <!--<priority value="ERROR" />-->
      <level value="ERROR" />
      <appender-ref ref="rollingFile" />
      <appender-ref ref="gdAppender" />
    </root>
  </log4net>

</configuration>